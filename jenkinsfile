pipeline
{
    agent any
    stages{
     stage('Build Application'){
     steps{
     bat 'mvn clean install'
     }
     }

     stage('Deploy Application to Mulesoft Cloudhub'){
     steps{
     bat 'mvn clean deploy -DmuleDeploy -DskipTests -Dmule.version=4.4.0 -Danypoint.username=7013311412 -Danypoint.password=Krish@136 -Denv=Sandbox -Dappname=myfirstproject -Dbusiness=Capgemini -DvCore=Micro -Dworkers=1 -DaltDeploymentRepository=myinternalrepo::default::file:///C:/snapshots'
     }
     }

    }
}